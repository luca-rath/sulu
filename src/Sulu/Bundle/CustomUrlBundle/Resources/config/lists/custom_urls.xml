<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>custom_urls</key>

    <properties>
        <property
            name="id"
            visibility="no"
            translation="sulu_admin.id"
            sortable="false"
        />
        <property
            name="title"
            visibility="always"
            translation="sulu_admin.title"
            sortable="false"
        />
        <property
            name="published"
            visibility="yes"
            translation="sulu_custom_url.published"
            sortable="false"
        >
            <transformer type="bool"/>
        </property>
        <property
            name="targetTitle"
            visibility="yes"
            translation="sulu_custom_url.target_title"
            sortable="false"
        />
        <property
            name="changerFullName"
            visibility="yes"
            translation="sulu_admin.changer"
            sortable="false"
        />
        <property
            name="changed"
            visibility="yes"
            translation="sulu_admin.changed"
            sortable="false"
        >
            <transformer type="datetime"/>
        </property>
        <property
            name="creatorFullName"
            visibility="yes"
            translation="sulu_admin.creator"
            sortable="false"
        />
        <property
            name="created"
            visibility="yes"
            translation="sulu_admin.created"
            sortable="false"
        >
            <transformer type="datetime"/>
        </property>
    </properties>
</list>
